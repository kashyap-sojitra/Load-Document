{
    "version": 2,
    "builds": [
      {
        "src": "dist/src/main.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/src/main.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }
  